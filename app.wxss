/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.view-contain-ti {
  display: flex;
  height: 90rpx;
  margin: 40rpx ;
  border: 3rpx solid #faca82;
  border-radius: 10rpx;
  background-color:#ffffff;
}

.text-ti {
  position: absolute;
  font-size: 12rpx;
  background: white;
  margin: -10rpx 0 0 10rpx;
  padding: 0 5rpx;
  color: rgb(144, 147, 167);
}

.input1 {
  margin-left: 10rpx;
  margin-bottom: 10rpx auto;
}

.page {
  background-color: #F5F5F5;
}